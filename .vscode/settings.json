{
  "editor.tabSize": 2,
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".env"
  ],
  "cSpell.ignoreWords": ["vuex", "firestore", "lymannhs"],
  "search.exclude": {
    "package-lock.json": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
